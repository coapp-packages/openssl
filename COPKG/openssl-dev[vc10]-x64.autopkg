// details for x64 version of openssl package
// build with: autopackage openssl-dev[vc10]-x64.autopkg
@import "openssl-dev.inc" ;

#define { 
    flavor: "vc10"; 
    arch : "x64";
}

files[libs] {
    root: @"..\out64dll\";
    
    include: {
        "libeay32.lib",
		"ssleay32.lib",
    };
    
    trim-path: all;
};
