// details for x86 version of openssl package
// build with: autopackage x86.autopkg openssl.autopkg  
@import "openssl.inc";

#define { 
    flavor: "vc10"; 
    arch : "x86";
}

files[exes] {
    root: @"..\out32dll\";
    
    include: {
        "openssl.exe",
    };
    
    trim-path: all;
};


files[dlls] {
    root: @"..\out32dll\";
  
    include: {
        "libeay32.dll",
		"ssleay32.dll",
    };
    
    trim-path: all;
};